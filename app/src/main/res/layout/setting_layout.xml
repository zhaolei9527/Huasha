<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/y38"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/img_back"
                android:layout_width="@dimen/x9"
                android:layout_height="@dimen/y15"
                android:layout_marginLeft="@dimen/x9"
                android:layout_marginTop="@dimen/y12"
                android:background="@mipmap/back" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:text="Setting Up"
                android:textColor="#333333"
                android:textSize="@dimen/x18" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#e0e0e0"></View>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/y8"
            android:background="#f7f7f7"></View>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#e0e0e0"></View>
        <RelativeLayout
            android:id="@+id/rl_changepassworld"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y38"
            android:orientation="horizontal">

            <TextView
                android:layout_marginLeft="@dimen/x9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="Change Password"
                android:textColor="#454545"
                android:textSize="@dimen/x18" />

            <ImageView

                android:layout_width="@dimen/x7"
                android:layout_height="@dimen/y10"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/y5"
                android:background="@mipmap/more" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#e0e0e0"></View>
        <RelativeLayout
            android:id="@+id/rl_update"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y38"
            android:orientation="horizontal">

            <TextView
                android:layout_marginLeft="@dimen/x9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="Version update"
                android:textColor="#454545"
                android:textSize="@dimen/x18" />

            <TextView
                android:id="@+id/tv_version"
                android:textColor="#454545"
                android:text="1.0.1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/y5"
               />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#e0e0e0"></View>
        <RelativeLayout
            android:id="@+id/rl_exit"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y38"
            android:orientation="horizontal">

            <TextView
                android:layout_marginLeft="@dimen/x9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="Log Out"
                android:textColor="#454545"
                android:textSize="@dimen/x18" />

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#e0e0e0"></View>
    </LinearLayout>
</FrameLayout>
